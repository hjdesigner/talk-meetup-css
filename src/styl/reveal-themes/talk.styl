.github-demo
	top: 0px !important;	
	@-webkit-keyframes efeitoBoca {
	0%  {width: 40px; height: 25px; bottom: -5px; left: 148px;}
	25% {width: 158px; height: 35px; bottom: -16px; left: 87px;}
	50% {width: 158px; height: 35px; bottom: -16px; left: 87px;}
	75% {width: 158px; height: 35px; bottom: -16px; left: 87px;}
	90% {width: 158px; height: 35px; bottom: -16px; left: 87px;}
	100% {width: 40px; height: 25px; bottom: -5px; left: 148px;}
	}
	@-webkit-keyframes efeitoBocaCima{
	0%{width: 40px; height: 0px; opacity: 1}
	25%{width: 40px; height: 0px; opacity: 0}
	30%{width: 40px; height: 0px; opacity: 0}
	90%{width: 40px; height: 25px; opacity: 0}
	100{width: 40px; height: 25px; opacity: 1}
	}
	@-webkit-keyframes efeitoArcoIris{
	0%{opacity: 0; height: 0px; }
	25%{opacity: 1; height:200px; }
	50%{opacity: 1; height:200px;}
	75%{opacity: 1; height:200px;}
	100%{opacity: 0; height:0px; bottom:00px;}
	}
