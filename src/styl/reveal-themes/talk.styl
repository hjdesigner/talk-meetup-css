	/*CABEÇA*/
	.github-rosto:after{
		content: "";
	    width: 371px;
	    height: 177px;
	    position: absolute;
	    left: 39px;
	    z-index: 2;
	    top: -16px;
	    background-color: #000;
	    border-radius: 50%;
	}
	/*QUEIXO*/
	.github-rosto:before {
	    content: "";
	    width: 286px;
	    height: 50px;
	    position: absolute;
	    left: 80px;
	    z-index: 1;
	    top: 295px;
	    background-color: #000;
	    border-radius: 50%;
	}
	/*ORELHAS*/
	.github-orelha{
		width: 180px;
	    height: 180px;
	    position: absolute;
	    z-index: 1;
	    background: #000;
	    border-radius: 150px 0;
	}
	/*ORELHA ESQUERDA*/
	.orelha-esquerda{
	    transform: rotate(90deg);
	    top: -40px;
	}
	/*ORELHA DIREITA*/
	.orelha-direita{
	    transform: rotate(180deg);
	    top: -40px;
	    right: 0px;
	}
